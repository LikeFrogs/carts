function _init()
	--initialize player
	player = {
		x = 16,
		y = 48
	}
end

function _update()
	if (btn(0)) then 
		player.x = player.x - 1 
	end
	if (btn(1)) then 
		player.x = player.x + 1 
	end
	if (btn(2)) then 
		player.y = player.y - 1 
	end
	if (btn(3)) then 
		player.y = player.y + 1 
	end
end

function _draw()
	cls(7)
	circfill(player.x, player.y, 10, 4)
end

